<ul>
  {{#each blogsData}}
    <li class="mb-5 rounded-sm border-4 border-dark">
      <a href="/api/blogs/{{id}}">
       <div class="flex h-11 items-center justify-between bg-dark px-5 text-light">
    <h2 class="font-semi-bold text-2xl">{{title}}</h2>
    <p>Posted by
      {{user.username}}
      on
      {{format_time created_at}}</p>
  </div>
  <p class="px-5 py-3 text-xl">{{content}}</p>
      </a>
    </li>
  {{/each}}
</ul>